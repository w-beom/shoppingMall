<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="shopping.mapper.BoardMapper">

	<insert id="insertCBoard" parameterType="shopping.vo.Customer_BoardVO">
		insert into customer(customer_subject,customer_content,customer_date)
		values(#{customer_subject},#{customer_content},sysdate)
	</insert>
	
</mapper>